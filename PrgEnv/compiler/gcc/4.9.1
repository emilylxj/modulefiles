setenv PRG_ENV gcc-4.9

set MODNAME gcc
set VERSION 4.9.1


proc ModulesHelp { } {
        puts stderr "\tgcc $VERSION - GNU C/C++ Progamming  Enviroment"
}
module-whatis "Programming Enviroment  GNU C/C++ $VERSION"


set prefix /pkg/$MODNAME/$VERSION
setenv GNUPATH $prefix 
prepend-path PATH $prefix/bin
prepend-path LIBRARY_PATH  $prefix/lib64
prepend-path MANPATH $prefix/share/man
prepend-path LD_LIBRARY_PATH  $prefix/lib64
prepend-path LD_RUN_PATH $prefix/lib64
prepend-path INCLUDE $prefix/include/c++/$VERSION


